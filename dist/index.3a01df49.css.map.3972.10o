\n    gap: 29px;\r\n}\r\n\r\n.head__container--rigth-side__ul li {\r\n    list-style: none;\r\n}\r\n\r\n.head__container--rigth-side__ul li a {\r\n    text-decoration: none;\r\n    color: white;\r\n    font-size: 20px;\r\n    font-size: 14px;\r\n    font-style: normal;\r\n    font-weight: 500;\r\n    line-height: 24px;\r\n}\r\n\r\n.head__container--rigth-side__ul li a:hover {\r\n    text-decoration: underline;\r\n}\r\n\r\n.nav-right {\r\n    display: flex;\r\n    gap: 20px;\r\n\r\n} */\r\n\r\n.menu-icon {\r\n  display: none;\r\n}\r\n\r\n/* Making dropdown contacts list  */\r\n#contact-link {\r\n  position: relative;\r\n}\r\n#contactData {\r\n  display: block;\r\n  position: absolute;\r\n  align-items: start;\r\n  justify-content: center;\r\n  background-color: rgba(255, 255, 255, 0.2);\r\n  color: var(--light-text-color);\r\n  border-radius: 8px;\r\n  top: 8%;\r\n  right: 5%;\r\n}\r\n\r\n.list-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  gap: 15px;\r\n  padding: 5px 5px;\r\n  cursor: pointer;\r\n  border-bottom: 0.5px solid var(--primary-color);\r\n}\r\n\r\n.postsContent {\r\n  display: none;\r\n}\r\n\r\n.postBlock {\r\n  display: flex;\r\n  position: absolute;\r\n  background-color: rgba(255, 255, 255, 0.4);\r\n  color: black;\r\n  justify-content: end;\r\n  overflow: hidden;\r\n  right: 0%;\r\n  top: 0%;\r\n  width: 100%;\r\n  height: 100vh;\r\n  gap: 20px;\r\n  z-index: 3;\r\n}\r\n.button-close {\r\n  position: absolute;\r\n  right: 510px;\r\n  margin-top: 5px;\r\n  border-radius: 8px;\r\n  border: none;\r\n  background-color: var(--primary-color);\r\n  color: var(--light-text-color);\r\n  padding: 15px 20px;\r\n  width: fit-content;\r\n  height: fit-content;\r\n  font-size: 1rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.posts-ul {\r\n  padding: 15px 15px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n  max-width: 450px;\r\n  width: 100%;\r\n  background-color: white;\r\n}\r\n.posts-list {\r\n  cu